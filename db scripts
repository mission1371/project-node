

CREATE SCHEMA `rest` DEFAULT CHARACTER SET utf8 ;

CREATE TABLE `rest`.`message` (
  `ID` VARCHAR(255) NOT NULL,
  `CREATED_DATE_TIME` VARCHAR(14) NOT NULL,
  `SEND_DATE_TIME` VARCHAR(14) NOT NULL,
  `MESSAGE_FROM` VARCHAR(100) NOT NULL,
  `MESSAGE_TO` VARCHAR(100) NOT NULL,
  `CONTENT` VARCHAR(1000) NULL,
  `IS_READ` INT(1) NOT NULL,
  PRIMARY KEY (`ID`)
);





